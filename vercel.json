{
    "version": 2,
    "builds": [
        {
            "src": "chatbot.py",
            "use": "@vercel/python",
            "config": {
                "pythonVersion": "3.12"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "chatbot.py"
        }
    ]
}
